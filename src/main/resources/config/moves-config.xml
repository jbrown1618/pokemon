<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean class="com.jbrown.pokemon.battle.DamagingMove">
        <constructor-arg name="name" value="Tackle"/>
        <constructor-arg name="type" value="NORMAL"/>
        <constructor-arg name="accuracy" value="100"/>
        <constructor-arg name="basePower" value="35"/>
    </bean>
</beans>