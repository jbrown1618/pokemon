<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="1"/>
        <constructor-arg name="name" value="Bulbasaur"/>
        <constructor-arg name="type1" value="GRASS"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>
</beans>