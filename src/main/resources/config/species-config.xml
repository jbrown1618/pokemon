    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="7"/>
        <constructor-arg name="name" value="Squirtle"/>
        <constructor-arg name="type1" value="GRASS"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="8"/>
        <constructor-arg name="name" value="Wartortle"/>
        <constructor-arg name="type1" value="WATER"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="9"/>
        <constructor-arg name="name" value="Blastoise"/>
        <constructor-arg name="type1" value="WATER"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

</beans>