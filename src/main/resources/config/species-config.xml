<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="1"/>
        <constructor-arg name="name" value="Bulbasaur"/>
        <constructor-arg name="type1" value="GRASS"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="2"/>
        <constructor-arg name="name" value="Ivysaur"/>
        <constructor-arg name="type1" value="GRASS"/>
        <constructor-arg name="type2" value="POISON"/>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="3"/>
        <constructor-arg name="name" value="Venusaur"/>
        <constructor-arg name="type1" value="GRASS"/>
        <constructor-arg name="type2" value="POISON"/>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="4"/>
        <constructor-arg name="name" value="Charmander"/>
        <constructor-arg name="type1" value="FIRE"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="5"/>
        <constructor-arg name="name" value="Charmeleon"/>
        <constructor-arg name="type1" value="FIRE"/>
        <constructor-arg name="type2"><null/></constructor-arg>
    </bean>

    <bean class="com.jbrown.pokemon.model.Species">
        <constructor-arg name="number" value="6"/>
        <constructor-arg name="name" value="Charizard"/>
        <constructor-arg name="type1" value="FIRE"/>
        <constructor-arg name="type2" value="FLYING"/>
